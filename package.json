    "webpack-dev-server": "^5.2.3"
  },
  "resolutions": {
    "semver": "^7.7.3"
  },
  "dependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/underscore": "^1.13.0",
    "classnames": "^2.5.1",
    "csstype": "^3.2.3",
    "file-saver": "^2.0.5",
    "react": "^19.2.4",
