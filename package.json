    "typescript": "^5.9.3",
    "underscore": "^1.13.7"
  },
  "overrides": {
    "@typescript-eslint/utils": "^8.18.2",
    "cheerio": "1.0.0",
    "read-package-json": "^7.0.1",
    "typescript": "^5.9.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "packages/*/src/**/*.{ts,tsx}",
      "packages/*/index.ts",
      "!packages/react-bootstrap-table-ng-example/**/*.{ts,tsx}"
    ],
    "roots": [
      "<rootDir>/packages"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "modulePaths": [
      "<rootDir>/packages/react-bootstrap-table-ng"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/test/**/*.test.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(sinon|@some-other-esm-package|cheerio))"
    ]
  },
  "packageManager": "yarn@4.12.0"
}
