  "jest": {
    "collectCoverageFrom": [
      "packages/*/src/**/*.{ts,tsx}",
      "packages/*/index.ts",
      "!packages/react-bootstrap-table-ng-example/**/*.{ts,tsx}"
    ],
    "roots": [
      "<rootDir>/packages"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "modulePaths": [
      "<rootDir>/packages/react-bootstrap-table-ng"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/test/**/*.test.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(sinon|@some-other-esm-package|cheerio))"
    ]
  }
