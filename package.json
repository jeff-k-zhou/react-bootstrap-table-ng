{
  "name": "react-bootstrap-table-ng-monorepo",
  "version": "5.19.1",
  "private": true,
  "description": "Rebuilt for react-bootstrap-table, react-bootstrap-table-next, react-bootstrap-table-nextgen",
  "workspaces": [
    "packages/*",
    "website"
  ],
  "main": "index.js",
  "scripts": {
    "clean:build": "find packages -type f \\( -name \"*.js\" -o -name \"*.js.map\" -o -name \"*.d.ts\" \\) -not -path \"*/node_modules/*\" -not -path \"*/react-bootstrap-table-ng-example/*\" -exec rm -f {} +",
    "build": "yarn clean:build && tsc && ./node_modules/.bin/gulp prod",
    "watch": "yarn clean:build && tsc -w",
    "lint": "eslint ./packages --max-warnings 0",
    "pretest": "yarn lint --cache",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "storybook:clean": "cd ./packages/react-bootstrap-table-ng-example && yarn gh-pages:clean",
    "storybook:build": "cd ./packages/react-bootstrap-table-ng-example && yarn gh-pages:build",
    "storybook:deploy": "cd ./packages/react-bootstrap-table-ng-example && yarn gh-pages:deploy",
    "storybook": "cd ./packages/react-bootstrap-table-ng-example && yarn storybook",
    "website:clean": "cd website && yarn clear",
    "website:build": "cd website && yarn clear && cd .. && yarn workspace react-bootstrap-table-ng-example gh-pages:clean && yarn workspace react-bootstrap-table-ng-example gh-pages:build && cp -R packages/react-bootstrap-table-ng-example/storybook-static/storybook website/static && cd website && yarn build",
    "website:deploy": "cd  website && yarn gh-pages:deploy",
    "website": "cd website && npm run serve",
    "release": "yarn install && yarn build && lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeff-k-zhou/react-bootstrap-table-ng.git"
  },
  "homepage": "https://jeff-k-zhou.github.io/react-bootstrap-table-ng/",
  "author": "Jeff Zhou",
  "contributors": [
    {
      "name": "Allen Fang",
      "email": "ayu780129@hotmail.com",
      "url": "https://github.com/AllenFang"
    },
    {
      "name": "Terrence Miao",
      "email": "terrence.miao@gmail.com",
      "url": "https://github.com/TerrenceMiao"
    },
    {
      "name": "Chun-MingChen",
      "email": "nick830314@gmail.com",
      "url": "https://github.com/Chun-MingChen"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeff-k-zhou/react-bootstrap-table-ng/issues"
  },
  "engines": {
    "node": ">=18"
  },
