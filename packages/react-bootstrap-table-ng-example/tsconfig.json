{
  "compilerOptions": {
    "target": "es2021",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "react-bootstrap-table-ng": [
        "../react-bootstrap-table-ng/index.ts"
      ],
      "react-bootstrap-table-ng-editor": [
        "../react-bootstrap-table-ng-editor/index.ts"
      ],
      "react-bootstrap-table-ng-filter": [
        "../react-bootstrap-table-ng-filter/index.ts"
      ],
      "react-bootstrap-table-ng-overlay": [
        "../react-bootstrap-table-ng-overlay/index.ts"
      ],
      "react-bootstrap-table-ng-paginator": [
        "../react-bootstrap-table-ng-paginator/index.ts"
      ],
      "react-bootstrap-table-ng-toolkit": [
        "../react-bootstrap-table-ng-toolkit/index.ts"
      ],
      "file-saver": [
        "node_modules/file-saver"
      ]
    }
  },
}